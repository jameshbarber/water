{
  "name": "@add-water/sdk",
  "version": "0.1.0",
  "description": "Typed client and React Query hooks for the add-water API",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
