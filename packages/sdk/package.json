  },
  "dependencies": {
    "openapi-fetch": "^0.12.4"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.59.0"
  },
  "devDependencies": {
    "@tanstack/react-query": "^5.59.0",
    "openapi-typescript": "^7.9.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "publishConfig": {
    "access": "public"
  }
}

